import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner Sc=new Scanner(System.in);
		int n=Sc.nextInt();
		int[]a=new int[n];
		int temp,i;
		for(i=0;i<n;i++){
		    a[i]=Sc.nextInt();
		}
		for(i=0;i<n/2;i++){
		    temp=a[i];
		    a[i]=a[n-i-1];
		    a[n-i-1]=temp;
		}
		System.out.println("Reversed array:");
		for(i=0;i<a.length;i++){
		    System.out.println(a[i]+" ");
		}
		
		
	}
}
